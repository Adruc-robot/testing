<tr id="<%= dom_id account %>">
  <!--<td><%#= account.id %></td>
  <td><%#= account.user_id %></td>-->
  <td><%= '*' + account.account_number %></td>
  <td><%= account.alias %></td>
  <td><%= link_to edit_account_path(account) do %><i class="bi bi-pencil"></i><% end %></td>
  <td><%= link_to [account], data: {turbo_method: :delete, turbo_confirm: "Delete account?"} do %><i class="bi bi-trash3"></i><% end %></td>
</tr>

