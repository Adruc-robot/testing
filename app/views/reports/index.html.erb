    <div class="accordion" id="reportsTwo">
        <div class="accordion-item">
            <h2 class="accordion-header mb-3 ">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">By Vendor</button>
            </h2>
            <div id="collapseTwo" class="accordion-collapse collapse show" data-bs-parent="#reportsTwo">
                <div><%= raw @byVendor[:pieChart] %></div>
                <div><%= raw @byVendor[:legend] %></div>
            </div>
        </div>
    </div>
    <div class="accordion" id="reportsThree">
