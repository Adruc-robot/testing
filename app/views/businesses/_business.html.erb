
<tr id="<%= dom_id business %>">
  <!--<td><%#= business.id %></td>
  <td><%#= business.user_id %></td>-->
  <td><%= business.name %></td>
  <td><%= business.address %></td>
  <td><%= business.phone_number %></td>
  <td><%= business.notes %></td>
  <td><%= link_to edit_business_path(business) do %><i class="bi bi-pencil"></i><% end %></td>
